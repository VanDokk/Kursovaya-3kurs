<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ResultController">
   <children>
      <Label layoutX="24.0" layoutY="14.0" text="Total processes" />
      <TextField fx:id="totalProcessesText" editable="false" layoutX="14.0" layoutY="32.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font></TextField>
      <TextField fx:id="rejectedProcessesText" editable="false" layoutX="250.0" layoutY="32.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font></TextField>
      <Label layoutX="250.0" layoutY="14.0" text="Rejected processes" />
      <TextField fx:id="completeProcessesText" editable="false" layoutX="485.0" layoutY="32.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font></TextField>
      <Label layoutX="482.0" layoutY="14.0" text="Complete processes" />
      <TextArea fx:id="coresCompleteProcessesText" layoutX="14.0" layoutY="117.0" prefHeight="200.0" prefWidth="256.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font></TextArea>
      <Label layoutX="64.0" layoutY="100.0" text="Completed processes by Cores" />
      <TextField fx:id="averageWaitingTactsText" editable="false" layoutX="132.0" layoutY="32.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="126.0" layoutY="14.0" text="Average waiting time" />
      <TextField fx:id="idleTimeText" editable="false" layoutX="368.0" layoutY="32.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="395.0" layoutY="14.0" text="Idle time" />
      <TextField fx:id="averageRunningTimeText" editable="false" layoutX="345.0" layoutY="200.0">
         <font>
            <Font name="Monospaced Regular" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="315.0" layoutY="183.0" text="Average process  running time" />
   </children>
</AnchorPane>
